{
	"compilerOptions": {
		"baseUrl": "..",
		"paths": {
			"#imports": ["./.bf6/imports"],
			"modlib": ["./node_modules/@bf6mods/sdk/dist/index.d.ts"]
		},
		"target": "ES2021",
		"module": "NodeNext",
		"moduleResolution": "nodenext",
		"strict": true,
		"allowImportingTsExtensions": true,
		"noEmit": true,
		"types": ["@bf6mods/sdk"]
	},
	"include": ["../**/*", "./bf6.d.ts"]
}
