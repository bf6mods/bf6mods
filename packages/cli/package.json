{
	"name": "@bf6mods/cli",
	"version": "1.3.0",
	"description": "CLI and library for bundling BF6 mods",
	"license": "MIT",
	"type": "module",
	"bin": {
		"bf6mods": "./dist/cli/index.js"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "tsdown",
		"start": "cross-env EXIT_CODE=none tsx ./src/cli/index.ts"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist/"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bf6mods/bf6mods.git"
	},
	"dependencies": {
		"@bf6mods/portal": "*",
		"@bf6mods/sdk": "*",
		"@clack/prompts": "^0.11.0",
		"chokidar": "^4.0.3",
		"colors": "^1.4.0",
		"commander": "^14.0.1",
		"jiti": "^2.6.1",
		"keytar": "^7.9.0",
		"knitwork": "^1.2.0",
		"rolldown": "^1.0.0-beta.44",
		"stringify-object": "^6.0.0",
		"strip-ansi": "^7.1.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@oxc-project/types": "^0.95.0",
		"@types/stringify-object": "^4.0.5",
		"cross-env": "^10.1.0",
		"execa": "^9.6.0",
		"puppeteer": "^24.26.1",
		"tsx": "^4.20.6"
	}
}
