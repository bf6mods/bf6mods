{
	"name": "@bf6mods/sdk",
	"version": "1.1.1",
	"description": "TypeScript definitions for the BF6 modding API",
	"type": "module",
	"license": "MIT",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./std": {
			"types": "./dist/std/index.d.ts",
			"import": "./dist/std/index.js"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bf6mods/bf6mods.git"
	},
	"files": [
		"dist/"
	],
	"scripts": {
		"build": "tsdown"
	},
	"keywords": [
		"bf6",
		"mods",
		"typescript",
		"definitions",
		"api"
	],
	"publishConfig": {
		"access": "public"
	}
}
